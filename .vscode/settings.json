{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.organizeImports": true
    },
    //"python.pythonpath": "",
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length=119"
    ],
    "python.sortImports.args": [
        "--profile=black"
    ]
}
